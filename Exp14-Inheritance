#include<iostream>
using namespace std;
class father{
    public:
        void house(){
            cout<<"I have a 2BHK house"<<endl;
        }

};
class son:public father{
    public:
        void car(){
            cout<<"I have a car"<<endl;
        }

};
int main(){
    son o;
    o.house();
    o.car();
    return 0;
}
